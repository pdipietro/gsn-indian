$("#graph-container").html("")
<% if @error_message.present? %>
  alert("<%= @error_message %>")
<% else %>
	graph_data = JSON.parse('<%= raw [@data_collections.to_json][0] %>');
	var node_ids=[]
	<% @check_node.each do |node| %> 
	  node_ids.push(<%= node %>)
	<% end %>

	load_graph(graph_data, node_ids)
<% end %>


